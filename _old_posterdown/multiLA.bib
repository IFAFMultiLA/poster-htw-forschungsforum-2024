@inproceedings{nakamura_development_2012,
	title = {{DEVELOPMENT} {OF} {A} {QUESTION}-{AUTHORING} {TOOL} {FOR} {MATH} {E}-{LEARNING} {SYSTEM} {STACK}},
	abstract = {STACK, System for Teaching and Assessment using a Computer algebra Kernel, is a system of a computer aided assessment package for mathematics and works as subsystem of Moodle, one of the Learning Management System (LMS). STACK can provide questions for a Moodle on-line test that inquires an answer as a mathematical expression and assess the answer algebraically. We have been developing Japanese version of STACK as a part of work of internationalization. STACK can not only assess the students’ answer correct or incorrect but also give some suitable feedbacks for the various students’ answer. Partial score can be given for incomplete answers. In order to use the rich function of STACK, we have to design a question carefully. However, one of the difficulties of STACK is authoring questions because the interface is inconvenient and we have to be on-line when we author questions. In order to overcome the problem, we have developed a question-authoring tool by the use of Excel and VBA. A question can be written on Excel interface and XML file is produced that can be imported to STACK question bank. Furthermore the tool can visualize a structure of STACK question, a potential response tree, on a browser. Compared to authoring questions on the existing web interface of STACK, it is expected that time spent on authoring questions can be reduced.},
	language = {en},
	author = {Nakamura, Yasuyuki and Ohmata, Yuka and Nakahara, Takahiro},
	year = {2012},
	keywords = {authoringtools, gelesen:MK, kurz, qualität:schlecht, domain:math, relevanz:niedrig},
	annote = {Kurzer Artikel speziell für AT für Mathematik (STACK). Bezieht sich nur auf die Entwicklung eines Excel/VBA Tools, um leichter Fragen mit passendem in STACK entwickeln zu können.
},
	file = {Nakamura et al. - 2012 - DEVELOPMENT OF A QUESTION-AUTHORING TOOL FOR MATH .pdf:C\:\\Users\\beinruck\\Zotero\\storage\\4GL42LPK\\Nakamura et al. - 2012 - DEVELOPMENT OF A QUESTION-AUTHORING TOOL FOR MATH .pdf:application/pdf},
